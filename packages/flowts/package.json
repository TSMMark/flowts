{
  "name": "flowts",
  "version": "0.12.5",
  "description": "Utility script to convert flow typed code to typescript",
  "bin": {
    "flowts": "./bin/flowts.js"
  },
  "scripts": {
    "cover": "jest --coverage",
    "test": "jest",
    "clean": "rimraf dist",
    "prepublishOnly": "yarn run clean && tsc -b --force",
    "build": "tsc -b",
    "ts-node": "ts-node"
  },
  "files": [
    "bin/*",
    "dist/*",
    "readme.md"
  ],
  "keywords": [],
  "author": "Bogdan Savluk <savluk.bogdan@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/zxbodya/flowts/tree/master/packages/cli",
  "dependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-flow-strip-types": "^7.14.5",
    "@babel/plugin-transform-typescript": "^7.15.4",
    "@babel/preset-flow": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/traverse": "^7.15.4",
    "@babel/types": "^7.15.6",
    "@flowts/babel-plugin-recast": "^0.12.5",
    "@zxbodya/babel-plugin-flow-to-typescript": "^0.12.5",
    "@zxbodya/recast": "0.20.9",
    "babel-plugin-typescript-to-flow": "^0.12.5",
    "commander": "^8.2.0",
    "globby": "^11.0.3",
    "jest-diff": "^27.1.1",
    "ora": "^5.4.1",
    "prettier": "^2.4.0",
    "source-map-support": "^0.5.20",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/preset-env": "^7.15.6",
    "@types/babel__core": "^7.1.16",
    "@types/babel__traverse": "7.14.2",
    "@types/glob": "^7.1.4",
    "@types/jest": "^27.0.1",
    "@types/ora": "^3.2.0",
    "@types/prettier": "^2.3.2",
    "jest": "^27.1.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "dist"
