{
  "name": "babel-plugin-typescript-to-flow",
  "description": "Babel plugin to convert TypeScript into Flow",
  "license": "MIT",
  "files": [
    "dist/*",
    "README.md"
  ],
  "version": "0.12.0",
  "scripts": {
    "cover": "jest --coverage",
    "test": "jest",
    "clean": "rimraf dist",
