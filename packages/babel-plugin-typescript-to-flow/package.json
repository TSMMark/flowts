{
  "name": "babel-plugin-typescript-to-flow",
  "description": "Babel plugin to convert TypeScript into Flow",
  "license": "MIT",
  "files": [
    "dist/*",
    "README.md"
  ],
  "version": "0.11.7",
  "scripts": {
    "cover": "jest --coverage",
    "report-coverage": "codecov",
    "test": "jest",
    "clean": "rimraf dist",
    "prepublishOnly": "yarn run clean && tsc -b --force",
    "build": "tsc -b"
  },
  "keywords": [
    "babel",
    "flow",
    "typescript"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/zxbodya/flowts/tree/master/packages/babel-plugin-typescript-to-flow",
  "peerDependencies": {
    "@babel/core": "^7.10.4"
  },
  "dependencies": {
    "@babel/template": "^7.12.7",
    "@babel/traverse": "^7.12.10",
    "@babel/types": "^7.12.11"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/babel__traverse": "7.11.0",
    "@types/jest": "^26.0.19",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "@zxbodya/recast": "^0.18.9",
    "codecov": "^3.8.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "dist"
    ]
  }
}
