{
  "name": "flowts",
  "version": "0.11.11",
  "description": "Utility script to convert flow typed code to typescript",
  "bin": {
    "flowts": "./bin/flowts.js",
    "tsflow": "./bin/tsflow.js"
  },
  "scripts": {
    "cover": "jest --coverage",
    "report-coverage": "codecov",
    "test": "jest",
    "clean": "rimraf dist",
    "prepublishOnly": "yarn run clean && tsc -b --force",
    "build": "tsc -b",
    "ts-node": "ts-node"
  },
  "files": [
    "bin/*",
    "dist/*",
    "readme.md"
  ],
  "keywords": [],
  "author": "Bogdan Savluk <savluk.bogdan@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/zxbodya/flowts/tree/master/packages/cli",
  "dependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-flow-strip-types": "^7.13.0",
    "@babel/plugin-transform-typescript": "^7.14.4",
    "@babel/preset-flow": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/traverse": "^7.14.2",
    "@babel/types": "^7.14.4",
    "@zxbodya/babel-plugin-flow-to-typescript": "^0.11.10",
    "@zxbodya/recast": "^0.20.4",
    "babel-plugin-typescript-to-flow": "^0.11.10",
    "commander": "^7.2.0",
    "globby": "^11.0.3",
    "jest-diff": "^27.0.2",
    "ora": "^5.4.0",
    "prettier": "^2.3.1",
    "source-map-support": "^0.5.19",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.4",
    "@babel/preset-env": "^7.14.4",
    "@types/babel__core": "^7.1.14",
    "@types/babel__traverse": "7.11.1",
    "@types/glob": "^7.1.2",
    "@types/jest": "^26.0.23",
    "@types/ora": "^3.2.0",
    "@types/prettier": "^2.2.3",
    "codecov": "^3.8.2",
    "jest": "^27.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "dist"
    ]
  }
}
