{
  "name": "flowts",
  "version": "0.11.3",
  "description": "Utility script to convert flow typed code to typescript",
  "bin": {
    "flowts": "./bin/flowts.js",
    "tsflow": "./bin/tsflow.js"
  },
  "scripts": {
    "cover": "jest --coverage",
    "report-coverage": "codecov",
    "test": "jest",
    "clean": "rimraf dist",
    "prepublishOnly": "yarn run clean && tsc -b --force",
    "build": "tsc -b",
    "ts-node": "ts-node"
  },
  "files": [
    "bin/*",
    "dist/*",
    "readme.md"
  ],
  "keywords": [],
  "author": "Bogdan Savluk <savluk.bogdan@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/zxbodya/flowts/tree/master/packages/cli",
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-flow-strip-types": "^7.10.4",
    "@babel/plugin-transform-typescript": "^7.11.0",
    "@babel/preset-flow": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/traverse": "^7.11.5",
    "@babel/types": "^7.11.5",
    "@zxbodya/babel-plugin-flow-to-typescript": "^0.11.3",
    "@zxbodya/recast": "^0.18.9",
    "babel-plugin-typescript-to-flow": "^0.11.3",
    "commander": "^6.1.0",
    "globby": "^11.0.1",
    "jest-diff": "^26.4.2",
    "ora": "^5.1.0",
    "prettier": "^2.1.2",
    "source-map-support": "^0.5.19",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@types/babel__core": "^7.1.10",
    "@types/babel__traverse": "7.0.15",
    "@types/glob": "^7.1.2",
    "@types/jest": "^26.0.14",
    "@types/ora": "^3.2.0",
    "@types/prettier": "^2.1.1",
    "codecov": "^3.7.2",
    "jest": "^26.4.2",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "dist"
    ]
  }
}
