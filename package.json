{
  "name": "flowts",
  "version": "0.4.7",
  "description": "Utility script to convert flowtyped code to typescript",
  "bin": {
    "flowts": "./bin/flowts.js"
  },
  "scripts": {
    "test": "jest",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p .",
    "ts-node": "ts-node"
  },
  "files": [
    "bin/*",
    "dist/*",
    "README.md"
  ],
  "keywords": [],
  "author": "Bogdan Savluk <savluk.bogdan@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-flow-strip-types": "^7.4.4",
    "@babel/plugin-transform-typescript": "^7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/traverse": "^7.4.5",
    "@zxbodya/babel-plugin-flow-to-typescript": "^0.5.9",
    "commander": "^2.20.0",
    "glob": "^7.1.4",
    "prettier": "^1.18.2",
    "@zxbodya/recast": "^0.18.1",
    "source-map-support": "^0.5.12"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.2",
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.15",
    "@types/prettier": "^1.16.4",
    "babel-plugin-tester": "^6.4.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.2"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
